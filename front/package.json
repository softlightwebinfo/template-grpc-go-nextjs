{
  "name": "template-go-nextjs",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "sass": "node-sass --importer node_modules/node-sass-import -w ./Framework/sass/index.scss -o ./public/static/css",
    "sass-build": "node-sass --importer node_modules/node-sass-import -w ./Framework/sass/index.scss -o ./public/static/css --output-style compressed",
    "tslint": "tslint -p . -c tslint.json"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.10.1",
    "@codeunic/library-ui": "^0.20.0",
    "@grpc/proto-loader": "^0.5.4",
    "@types/express": "^4.17.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cross-env": "^5.2.0",
    "es6-promise": "^4.2.8",
    "express": "latest",
    "express-session": "^1.17.1",
    "grpc": "^1.24.3",
    "grpc-tools": "^1.9.1",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.29.2",
    "mongoose": "^5.9.21",
    "multer": "^1.4.2",
    "next": "latest",
    "next-redux-saga": "^4.1.2",
    "next-redux-wrapper": "^6.0.1",
    "next-routes": "^1.4.2",
    "node-grpc-client": "^1.4.0",
    "node-sass": "^4.14.1",
    "node-sass-import": "^2.0.1",
    "nookies": "^2.3.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-image-gallery": "^1.0.7",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.1.3",
    "sharp": "^0.25.3",
    "ts-protoc-gen": "^0.12.0",
    "tslint-react-hooks": "^2.2.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "nodemon": "^1.19.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.1.0",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.4.5"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  }
}
